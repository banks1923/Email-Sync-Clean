[run]
branch = True
source = gmail_service,search_service,vector_service
omit =
    */tests/*
    */__init__.py
    */conftest.py
    setup_credentials.py
    */migrations/*
    */venv/*
    */.venv/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
