{
  "include": [
    "shared",
    "gmail", 
    "pdf",
    "transcription",
    "entity",
    "summarization",
    "utilities",
    "infrastructure"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "tests",
    "data",
    "tools/scripts"
